<div class="container">
  <center>
    <img src="{{ '/img/links.svg' | url }}"  width="90px" height="90px" alt="Links Icon" />
  </center>
</div>
<div style="margin: 10px;"></div>
<div class="container text-start">

<div class="container text-center">
  <div class="row">
    {%- for linkki in linkitsivu %}
    <div class="col-md-4 linkitwrapper">
      <h1 style="font-size: 1.5em">{{ linkki.fields.title }}</h1>
        {%- if linkki.sys.id == "63NyOESX2erzjbcJbKYCjT" or linkki.sys.id == "5wefvZkZrxW9Ael0EctJVE" or linkki.sys.id == "W1zEqU0Bcqf6yeIoD75M3" %}
        <div class="well youtube">
          {{ linkki.fields.content | renderRichTextWithAssets | safe }}
        </div>
        {% else %} 
        <div class="well">
          {{ linkki.fields.content | renderRichTextWithAssets | safe }}
        </div>
        {% endif %}
    </div>
    {% endfor -%}
  </div>
</div>